---
import { Card, CardGrid, Icon } from "@astrojs/starlight/components";
import {} from "@astrojs/starlight";

import { getCollection, getEntry } from "astro:content";

const guides = await getCollection("docs", (e) => e.slug.includes("guides"));
---

<CardGrid>
  {guides.map((guide) => <Card title={guide.data.title} icon="open-book" />)}
</CardGrid>
